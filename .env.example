# ===========================================
# Vestige Configuration
# ===========================================
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# -------------------------------------------
# Required
# -------------------------------------------

# Your OpenRouter API key (https://openrouter.ai/keys)
OPENROUTER_API_KEY=

# Redis authentication
REDIS_PASSWORD=

# Memgraph authentication  
MEMGRAPH_USER=
MEMGRAPH_PASSWORD=

# Your display name (creates the root user entity)
VESTIGE_USER_NAME=

# -------------------------------------------
# Optional - defaults shown
# -------------------------------------------

# Data store hosts
REDIS_HOST=localhost
REDIS_PORT=6379
MEMGRAPH_HOST=localhost
MEMGRAPH_PORT=7687

# Model selection (OpenRouter model strings)
STRUCTURED_MODEL=google/gemini-2.5-flash
REASONING_MODEL=google/gemini-3-flash-preview
AGENT_MODEL=anthropic/claude-sonnet-4.5